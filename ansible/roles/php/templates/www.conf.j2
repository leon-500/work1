[php]

listen = 127.0.0.1:9000
listen.owner = php_fpm_owner
listen.group = php_fpm_group
listen.mode = 0660

user  = php_fpm_owner
group = php_fpm_group

pm = static
pm.max_children = 20
pm.max_requests = 5000
pm.process_idle_timeout = 20s
pm.status_path = /php-status

catch_workers_output = yes